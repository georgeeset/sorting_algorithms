#include <stdio.h>
#include <stdlib.h>
#include "sort.h"

/**
 * main - Entry point
  *
   * Return: Always 0
    */
    int main(void)
    {
            int array[] = {99, 23, 134, 88, 12, 9, 357, 778, 800, 124, 12, 133};
            size_t n = sizeof(array) / sizeof(array[0]);

            print_array(array, n);
            printf("\n");
            selection_sort(array, n);
            printf("\n");
            print_array(array, n);
            return (0);
    }
